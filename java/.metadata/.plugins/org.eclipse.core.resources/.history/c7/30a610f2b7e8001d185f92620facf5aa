
public class Controller {
	public boolean isValidAndControl(Model model) {
		try {
			int intData = Integer.parseInt(model.getStringInputData());
			if(!(0<intData&&intData<9)){
				return false;
			}
			model.setInputData(intData);
		} catch (NumberFormatException e) {
			return false;
		}
		return true;
	}
}

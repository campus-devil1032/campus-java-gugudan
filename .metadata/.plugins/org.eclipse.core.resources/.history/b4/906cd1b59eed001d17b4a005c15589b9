package NewCalculator;

public class NewGuGuDanController {
	private NewGuGuDanModel model;
	private NewGuGuDanView view;
	
	private NewGuGuDanController(NewGuGuDanModel model, NewGuGuDanView view) {
		this.model = model;
		this.view = view;
		
	}
	
	public void run() {
		int num = view.getInput();
		model.setNum(num);
		int[] table = model.calculateTable();
		view.result(table);
	}


}

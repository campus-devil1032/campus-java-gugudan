package CaclulatorWithGuGuDan;

import java.util.Scanner;

public class dddf {

	public static void main(String[] args) {
		
		while (ture) {
		Scanner sc = new Scanner(System.in);

		System.out.println("첫 번째 문자열을 입력해주세요");
		String userInputFirst = sc.nextLine();
		System.out.println("두 번째 문자열을 입력해주세요");
		String userInputSecond = sc.nextLine();
		System.out.println("세 번째 문자열을 입력해주세요");
		String userInputThird = sc.nextLine();

		System.out.println("userInputFirst : " + userInputFirst);
		System.out.println("userInputSecond : " + userInputSecond);
		System.out.println("userInputThird : " + userInputThird);

	}
	}

